{
    "version": 2,
    "builds": [
        {
            "src": "api/index.py",
            "use": "@vercel/python"
        }
    ],
    "routes": [
        {
            "src": "/v1/(.*)",
            "dest": "api/index.py"
        },
        {
            "src": "/docs",
            "dest": "api/index.py"
        },
        {
            "src": "/redoc",
            "dest": "api/index.py"
        },
        {
            "src": "/ping",
            "dest": "api/index.py"
        },
        {
            "src": "/openapi.json",
            "dest": "api/index.py"
        },
        {
            "src": "/(.*)",
            "dest": "api/index.py"
        }
    ]
}